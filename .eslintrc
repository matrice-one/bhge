{
    "parser": "@typescript-eslint/parser",
    "extends": ["plugin:react/recommended"],
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "react/react-in-jsx-scope": "off",
      "no-unused-vars": "error"
    }
  }
  